<!DOCTYPE aspectwerkz PUBLIC
    "-//AspectWerkz//DTD//EN"
    "http://aspectwerkz.codehaus.org/dtd/aspectwerkz.dtd">

<aspectwerkz>
    <system id="eworld/wlw/aop">
        <prepare package="eworld.service."/>
        <aspect class="eworld.service.ComputationStandalone$CacheAspect">
            <pointcut name="cache" expression="execution(* foo.*(..))"/>
            <advice name="cache" type="around" bind-to="cache"/>
        </aspect>
        <aspect class="eworld.service.ComputationStandalone$TraceAspect">
            <pointcut name="trace" expression="execution(* foo.*(..))"/>
            <advice name="trace" type="around" bind-to="trace"/>
        </aspect>
    </system>
</aspectwerkz>
