<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>AOSDServlet</servlet-name>
        <servlet-class>aspectwerkz.aosd.web.HomeServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>InitServlet</servlet-name>
        <servlet-class>aspectwerkz.aosd.web.InitServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>AOSDServlet</servlet-name>
        <url-pattern>/demo.do</url-pattern>
    </servlet-mapping>

</web-app>
