/**
 * Field modifier.
 */
void FieldModifier() #Modifier : {}
{
    (<FIELD_NOT>
    {
        jjtThis.toggleNot();
    })*
    (
    <FIELD_PUBLIC>
    {
        jjtThis.setModifier(Modifier.PUBLIC);
    }
    |
    <FIELD_PROTECTED>
    {
        jjtThis.setModifier(Modifier.PROTECTED);
    }
    |
